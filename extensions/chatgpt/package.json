{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chatgpt",
  "title": "ChatGPT",
  "description": "Have a conversation with ChatGPT",
  "icon": "chatgpt-icon.png",
  "author": "domnantas",
  "categories": [
    "Fun",
    "Developer Tools",
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "ChatGPTSessionToken",
      "title": "ChatGPT Session Token",
      "description": "Click About This Extension to learn how to obtain it",
      "type": "password",
      "required": true
    }
  ],
  "commands": [
    {
      "name": "index",
      "title": "Prompt ChatGPT",
      "subtitle": "OpenAI",
      "description": "Have a conversation with ChatGPT",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "@raycast/utils": "^1.4.11",
    "chatgpt": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.27.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
